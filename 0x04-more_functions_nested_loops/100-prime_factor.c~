#include <stdio.h>
#include <math.h>

/**
 * main - Entry point
 * Return: Always 0 (Success)
 */
int main(void)
{
long long i;
long long max = -1;
long long n = 612852475143;
while (n % 2 == 0)
{
max = 2;
n /= 2;
}
for (i = 3; i < sqrt(n); i += 2)
{
if (n % i == 0)
{
if (max < i)
max = i;
n /= i;
}
}
if (n > 2)
max = n;
printf("%d\n", max);
return (0);
}
